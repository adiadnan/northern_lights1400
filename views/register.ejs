<!DOCTYPE html>
<html lang="en">
<head>		
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">		
	<% include partials/default.ejs %>
	<link rel="stylesheet" href="/stylesheets/login.css">
	<script src="/javascripts/authentication.js"></script>		
</head>
<body>
	<!-- <% include partials/fbjsapp.ejs %> -->


	<div class="container">
		<div class="row">
			<div class="col-sm-6 col-md-4 col-md-offset-4">
				<div class="account-wall">
					<div id="my-tab-content" class="tab-content">
						<div class="tab-pane active" id="register">
							<div class="auth_title_g">
								<span id="auth_ti_format">
									Register
								</span>
							</div>
							<form class="form-signin" action="" method="">
								<input type="email" class="form-control bottom-spacing" placeholder="Email Address..." required>
								<input type="password" class="form-control bottom-spacing" placeholder="Password..." required>
								<input type="password" class="form-control bottom-spacing" placeholder="Confirm password..." required>
								<input type="submit" class="btn btn-lg btn-default btn-block bottom-spacing" value="Sign Up" onclick="register()" />
							</form>
							<div id="tabs" data-tabs="tabs">
									<a href="/login">
										Have an Account?
									</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		</html>